n1 = int(input("Digite o primeiro num"))
n2 = int(input("Digite o segundo num "))
opp = int(input("Digite 1 para soma, 2 para subtração, 3 para divisão, 4 para soma"))
mult = n1*n2
sub = n1-n2
div = n1/n2
soma = n1+n2
if opp == 1:
    print(mult)
elif opp == 2:
    print(sub)
elif opp == 3:
    print(div)
elif opp == 4:
    print(soma)
else: 
    print("Operaçao inválida")
